<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomBottomNavBarStyle">
        <item name="android:background">@color/light_gray</item>
        <item name="itemBackground">@drawable/navbar_new</item>
    </style>
    <style name="FullScreenTransparentDialog" parent="ThemeOverlay.MaterialComponents.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:fitsSystemWindows">false</item>
    </style>
</resources>